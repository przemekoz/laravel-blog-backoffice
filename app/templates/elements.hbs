
<Wrapper::Card @header="List of Elements" @mode="list" @createLabel="Add Element" @createRoute="element-create">

  <Wrapper::List
    @modelName="element"
    @filterType={{this.filterType}}
    @sortOptions={{this.sortOptions}}
    @meta={{@model.meta}}
    @data={{@model.data}}
  />

</Wrapper::Card>